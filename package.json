    "tld": "0.0.2"
  },
  "devDependencies": {
    "concat-stream": "^2.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
